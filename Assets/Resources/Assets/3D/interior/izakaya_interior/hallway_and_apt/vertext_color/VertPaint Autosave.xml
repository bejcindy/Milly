<?xml version="1.0" encoding="utf-8"?>
<vertPaintTemplate version="1.1.1">
  <enabled>false</enabled>
  <togglePreview>false</togglePreview>
  <hideTransformHandle>true</hideTransformHandle>
  <brushSettings>
    <radius>0.4</radius>
    <maxRadius>10</maxRadius>
    <delay>0.025</delay>
    <opacity>1</opacity>
    <falloff>
      <key>
        <time>0</time>
        <value>1</value>
        <inTangent>0</inTangent>
        <outTangent>-1</outTangent>
      </key>
      <key>
        <time>1</time>
        <value>0</value>
        <inTangent>-1</inTangent>
        <outTangent>0</outTangent>
      </key>
    </falloff>
    <color>
      <r>0</r>
      <g>0</g>
      <b>1</b>
      <a>1</a>
    </color>
    <style>2</style>
    <alpha>0.2</alpha>
    <blink>true</blink>
  </brushSettings>
  <keyBindings>
    <paint>Mouse0</paint>
    <preview>C</preview>
    <modifyRadius>X</modifyRadius>
  </keyBindings>
  <templates>
    <autosaveDirectory>Assets/Resources/Assets/3D/interior/izakaya_interior/hallway_and_apt/vertext_color/</autosaveDirectory>
  </templates>
  <meshOutputDirectory>Assets/Resources/Assets/3D/interior/izakaya_interior/hallway_and_apt/vertext_color/</meshOutputDirectory>
  <foldouts>
    <help>false</help>
    <brushSettings>true</brushSettings>
    <keyBindings>true</keyBindings>
    <templates>true</templates>
  </foldouts>
</vertPaintTemplate>